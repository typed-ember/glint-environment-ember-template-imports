{
  "name": "glint-environment-ember-template-imports",
  "version": "0.0.3",
  "repository": "typed-ember/glint-environment-ember-template-imports",
  "description": "A Glint environment to support ember-template-imports projects",
  "license": "MIT",
  "author": "Dan Freeman (https://github.com/dfreeman)",
  "glint-environment": "-private/environment",
  "main": "-private/index.js",
  "types": "-private/index.d.ts",
  "keywords": [
    "glint-environment"
  ],
  "scripts": {
    "lint": "eslint . --max-warnings 0 && prettier --check .",
    "test": "tsc --project __tests__",
    "build": "tsc --build",
    "prepack": "yarn build"
  },
  "files": [
    "README.md",
    "-private/**/*.{js,d.ts}"
  ],
  "peerDependencies": {
    "@glint/environment-ember-loose": "^0.7.5",
    "ember-template-imports": "^2.0.0"
  },
  "devDependencies": {
    "@glint/config": "^0.7.5",
    "@glint/core": "^0.7.5",
    "@glint/environment-ember-loose": "^0.7.5",
    "@glint/template": "^0.7.5",
    "@glint/transform": "^0.7.5",
    "@types/ember": "^4.0.0",
    "@types/ember__helper": "^4.0.0",
    "@types/ember__modifier": "^4.0.0",
    "ember-template-imports": "^2.0.0",
    "expect-type": "0.11.0",
    "typescript": "^4.5.5"
  },
  "prettier": {
    "singleQuote": true
  }
}
